         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="closebutton" layoutX="64.0" layoutY="473.0" mnemonicParsing="false" onAction="#closeButtonOnAction" prefHeight="25.0" prefWidth="336.0" style="-fx-background-color: RED;" text="cancel" textFill="#f2ebeb">
