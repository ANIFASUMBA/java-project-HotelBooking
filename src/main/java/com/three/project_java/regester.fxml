                  <Image url="../../../../../../walk.jpeg" />
               </image>
            </ImageView>
            <Label layoutX="87.0" layoutY="23.0" text="user regestration">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="64.0" layoutY="117.0" text="firstname" />
      <TextField fx:id="firstnametextfield" layoutX="64.0" layoutY="134.0" prefHeight="27.0" prefWidth="336.0" promptText="firstname" />
      <Label layoutX="64.0" layoutY="173.0" text="lastname" />
      <TextField fx:id="usernametextfield" layoutX="64.0" layoutY="251.0" prefHeight="27.0" prefWidth="336.0" promptText="username" />
      <Label layoutX="64.0" layoutY="227.0" text="username" />
      <TextField fx:id="lastnametextfield" layoutX="64.0" layoutY="194.0" prefHeight="27.0" prefWidth="336.0" promptText="lastname" />
